{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@app/*": ["./app/*", "./app/"],
      "@theme": ["./app/assets/ts/theme.ts"],
      "@assets/*": ["./app/assets/*", "./app/assets/"],
      "@css/*": ["./app/assets/css/*", "./app/assets/css/"],
      "@images/*": ["./app/assets/images/*", "./app/assets/images/"],
      "@tsStyles/*": ["./app/assets/ts/*", "./app/assets/ts/"],
      "@components/*": ["./app/components/*", "./app/components/"],
      "@constants/*": ["./app/constants/*", "./app/constants/"],
      "@containers/*": ["./app/containers/*", "./app/containers/"],
      "@views/*": ["./app/views/*", "./app/views/"],
      "@gitlab/*": ["./app/gitlab/*", "./app/gitlab/"],
      "@private/*": ["./app/private/*", "./app/private/"],
      "@routes/*": ["./app/routes/*", "./app/routes/"],
      "@state/*": ["./app/state/*", "./app/state/"],
      "@ducks/*": ["./app/state/ducks/*", "./app/state/ducks/"],
      "@middlewares/*": [
        "./app/state/middlewares/*",
        "./app/state/middlewares/"
      ],
      "@stateUtils/*": ["./app/state/utils/*", "./app/state/utils/"]
    }
  }
}
